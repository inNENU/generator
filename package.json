{
  "name": "innenu-generator",
  "version": "0.6.4",
  "description": "inNENU 生成器",
  "repository": {
    "type": "git",
    "url": "git@github.com:inNENU/generator.git"
  },
  "license": "Mr.Hope LICENSE",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./typings": {
      "types": "./typings.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "typings.d.ts"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf dist typings.d.ts",
    "lint": "eslint . --fix && pnpm lint:prettier --write",
    "lint:check": "eslint . && pnpm lint:prettier",
    "lint:prettier": "prettier --check .",
    "prepare": "husky",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "vitest"
  },
  "nano-staged": {
    "**/*": "prettier --write -u",
    "*.{js,ts}": "eslint --fix"
  },
  "dependencies": {
    "ali-oss": "^6.23.0",
    "js-yaml": "^4.1.0",
    "upath": "^2.0.1",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@types/ali-oss": "6.16.11",
    "@types/js-yaml": "4.0.9",
    "@types/node": "24.3.0",
    "eslint": "9.34.0",
    "eslint-config-mister-hope": "0.9.6",
    "husky": "9.1.7",
    "nano-staged": "0.8.0",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "rollup": "4.49.0",
    "rollup-plugin-dts": "6.2.3",
    "rollup-plugin-esbuild": "6.2.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.15.0"
}
